.pro_left {
  flex: 1;
  margin-left: 20rpx;
}
.pro_right {
  width: 80rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #3296FA;
}
.pro_img {
  width: 80rpx;
}
.pro_img image {
  width: 100%;
  height: 100%;
}
/* 一级文字颜色 */
.text_c_1 {
  color: #191F25;
}
/* 二级文字颜色 */
