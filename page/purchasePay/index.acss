@import "/mixin/loading/loading.acss";
.container {
  flex: 1;
  overflow: auto
}
.bank {
  background: #fff;
}
.page_footer {
    height: 120rpx;
    display: flex;
    padding-top: 20rpx;
    box-sizing: border-box;
    text-align: center;
    line-height: 100rpx;
}
.btn_left {
    width: 150rpx;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
}
.circle {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  line-height: 20rpx;
  border: 1px solid #ddd;
}
.btn_center {
    flex: 1;
    background: #fff;
    border-left: 1px solid #ddd;
    border-right: 1px solid #ddd;
}
.btn_right {
    flex: 1;
    background: #49a9ee;
    color: #fff;
}

.page_wrap {
  /* display: flex;
  flex-direction: column;
  height: 100vh; */
}
.m_20 {
  margin: 20rpx;
}
.page_header {
  margin-bottom: 20rpx;
  background: #fff;
}
.page_nav {
  height: 60rpx;
  margin: 20rpx;
  box-sizing: border-box;
  line-height: 60rpx;
  border-radius: 20rpx;
  overflow: hidden;
  border: 1px solid #ddd;
  display: flex;
  justify-content: space-between;
}
.nav_left {
  flex: 1;
  text-align: center;
}
.nav_right {
  flex: 1;
  text-align: center;
}
.params {
  background: #409EFF;
  color: #fff;
}
.pro_box {
  background: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  width: 100%;
}
/* .pro_box_center {
  display: flex;
  flex-direction: column;
} */

.pro_content {
  display: flex;
  flex-wrap: wrap;
}
