}
.submit {
    background: #3296FA;
    color: #fff;
    width: 100%;
}
.addView_cell_details_addBox {
  width: 150rpx;
  display: flex;
  flex-direction: row-reverse;
}
.addBoxItem {
  /* flex: 1; */
